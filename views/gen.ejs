    <style>
        #main {
            padding: 10px;
        }
        
        th {
            border: solid black 1px;
        }
        
        table {
            border: solid black 1px;
            margin: 0 auto;
        }
        
        td {
            margin: 10px;
            padding: 20px;
            border-right: 1px solid black;
        }
    </style>
    <script>
        $(document).ready(function() {
            $("#name").hide();
            $("#dates").hide();

            $("#nameB").click(function() {
                $("#dates").hide(100);
                $("#name").toggle(300);
                // $("#calendar").hide()
            });
            $("#dateB").click(function() {
                $("#name").hide();

                $("#dates").toggle(300);
                // $("#calendar").hide()
            });
        })
        var data = [
            [{
                "ename": "iOS Help Session",
                "location": "CH 3600",
                "Days": ["M", "W", "F"],
                "Time": "2: 00 pm - 2: 50 pm",
                "Attendance": "36 / 50"
            }],
            [{
                "ename": "Cyber Security Research Session",
                "location": "CH 3650",
                "Days": ["M", "W", "F"],
                "Time": "3: 00 pm - 3: 50 pm",
                "Attendance": "42 / 50"
            }, {
                "ename": "Business Analytics Project Meeting",
                "location": " CH 3600",
                "Days": "M",
                "Time": " 2: 00 pm - 2: 50 pm",
                "Attendance": "38 / 50"
            }, {
                "ename": "DBMS Help Session",
                "location": "Auditorium",
                "Days": "M",
                "Time": "10: 00 am - 5: 00 pm",
                "Attendance": "30 / 60"
            }]
        ]

        function load() {
            var tbody = document.getElementById('table')
            data[1].forEach(e => {
                var tr = document.createElement('tr')
                tr.innerHTML = `
                    <td>${e.ename}</td>
                    <td>${e.location}</td>
                    <td>${e.Days}</td>
                    <td>${e.Time}</td>
                    <td>${e.Attendance}</td>
                `
                tbody.appendChild(tr)
            })
        }
    </script>

    <body onload="load()">
        <%- include('./Components/navbar') %>
            <div id="main">
                <label for="">Enter Event name</label>
                <input type="text" name="" id="">
                <button class="btn btn-primary" id="nameB">Generate</button>
                <br>
                <h3>OR</h3>
                <label for="">Start Date</label>
                <input type="date" name="" id="">
                <label for="">End Date</label>
                <input type="date" name="" id="">
                <button class="btn btn-primary" id="dateB">Generate</button>
            </div>
            <div id="name">
                <table>
                    <thead>
                        <th>Ename</th>
                        <th>location</th>
                        <th>Day</th>
                        <th>Time</th>
                        <th>Day</th>
                        <th>Attendence</th>
                    </thead>
                    <tbody>
                        <tr>
                            <td>
                                EN Guest Lecture
                            </td>
                            <td>
                                CH 3600
                            </td>
                            <td>
                                M W F
                            </td>
                            <td>
                                2: 00 pm - 2: 50 pm
                            </td>
                            <td>
                                36 / 50
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div id="dates">
                <table>
                    <thead>
                        <th>Ename</th>
                        <th>location</th>
                        <th>Days</th>
                        <th>Time</th>
                        <th>Attendence</th>
                    </thead>
                    <tbody id="table">
                        <tr>
                            <td>
                                EN Guest Lecture
                            </td>
                            <td>
                                CH 3600
                            </td>
                            <td>
                                M W F
